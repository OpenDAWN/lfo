{
  "name": "src-audio",
  "version": "1.0.0",
  "description": "Audio source LFOs",
  "main": "src-audio.js",
  "scripts": {
    "build-audio-in": "babel -t lib/audio-in.es6.js | sed -e 's/  _class/  \\/\\/_class/g' > lib/audio-in.js",
    "build-node"    : "babel -t lib/audio-in-node.es6.js | sed -e 's/  _class/  \\/\\/_class/g' > lib/audio-in-node.js",
    "build-buffer"  : "babel -t lib/audio-in-buffer.es6.js | sed -e 's/  _class/  \\/\\/_class/g' > lib/audio-in-buffer.js",
    "build-modules": "npm run build-audio-in && npm run build-node && npm run build-buffer",
    "build": "npm run build-modules && browserify ./src-audio.js -t babelify --outfile src-audio-bundle.js",
    "clean-modules": "rm lib/audio-in-buffer.js && rm lib/audio-in-node.js && rm lib/audio-in.js",
    "clean": "npm run clean-modules rm src-audio-bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": ["brfs"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wavesjs/lfo"
  },
  "author": "Victor Saiz",
  "license": "BSD-3",
  "bugs": {
    "url": "https://github.com/wavesjs/lfo/issues"
  },
  "homepage": "https://github.com/wavesjs/lfo",
  "devDependencies": {
    "babel": "^4.0.1",
    "babelify": "^5.0.3",
    "brfs": "^1.3.0",
    "browserify": "^8.1.3"
  }
}